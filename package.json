{
  "name": "webnowplaying-vscode",
  "displayName": "WebNowPlaying-VSCode",
  "description": "Shows current music using WebNowPlaying-Redux",
  "version": "0.0.2",
  "publisher": "ne0ekspert",
  "repository": {
    "type": "git",
    "url": "https://github.com/ne0ekspert/webnowplaying-vscode.git"
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [ "onStartupFinished" ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wnpvscode.prevMusic",
        "title": "WebNowPlaying-VSCode: Previous Music"
      },
      {
        "command": "wnpvscode.togglePlayMusic",
        "title": "WebNowPlaying-VSCode: Play/Pause Music"
      },
      {
        "command": "wnpvscode.nextMusic",
        "title": "WebNowPlaying-VSCode: Next Music"
      }
    ],
    "configuration":[
      {
        "id": "wnpsettings",
        "title": "WebNowPlaying",
        "properties": {
          "WebNowPlaying.port": {
            "type": "number",
            "default": 1234
          },
          "WebNowPlaying.alignToRight": {
            "type": "boolean",
            "default": false
          }
        }
      }
    ]
  },
  "extensionKind": [
    "ui"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/ws": "^8.5.4",
    "edge": "^7.10.1",
    "ws": "^8.13.0"
  }
}
